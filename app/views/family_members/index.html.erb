<h1>Listing family_members</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Family</th>
    <th>Login</th>
    <th>Email</th>
    <th>Crypted password</th>
    <th>Password salt</th>
    <th>Persistence token</th>
    <th>Login count</th>
    <th>Failed login count</th>
    <th>Last request at</th>
    <th>Current login at</th>
    <th>Last login at</th>
    <th>Current login ip</th>
    <th>Last login ip</th>
  </tr>

<% @family_members.each do |family_member| %>
  <tr>
    <td><%=h family_member.name %></td>
    <td><%=h family_member.family_id %></td>
    <td><%=h family_member.login %></td>
    <td><%=h family_member.email %></td>
    <td><%=h family_member.crypted_password %></td>
    <td><%=h family_member.password_salt %></td>
    <td><%=h family_member.persistence_token %></td>
    <td><%=h family_member.login_count %></td>
    <td><%=h family_member.failed_login_count %></td>
    <td><%=h family_member.last_request_at %></td>
    <td><%=h family_member.current_login_at %></td>
    <td><%=h family_member.last_login_at %></td>
    <td><%=h family_member.current_login_ip %></td>
    <td><%=h family_member.last_login_ip %></td>
    <td><%= link_to 'Show', family_member %></td>
    <td><%= link_to 'Edit', edit_family_member_path(family_member) %></td>
    <td><%= link_to 'Destroy', family_member, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New family_member', new_family_member_path %>